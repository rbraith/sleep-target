<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <TextView
        android:id="@+id/session_details_overlap_start_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:layout_marginLeft="24dp"
        android:layout_marginTop="16dp"
        android:text="start:"
        android:textColor="?attr/colorOnPrimarySurface"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/session_details_overlap_message" />

    <TextView
        android:id="@+id/session_details_overlap_end_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        android:layout_marginBottom="8dp"
        android:text="end:"
        android:textColor="?attr/colorOnPrimarySurface"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="@+id/session_details_overlap_start_text"
        app:layout_constraintTop_toBottomOf="@+id/session_details_overlap_start_text" />

    <androidx.constraintlayout.widget.Barrier
        android:id="@+id/session_details_overlap_session_barrier"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:barrierDirection="right"
        app:constraint_referenced_ids="session_details_overlap_end_text,session_details_overlap_start_text"
        tools:layout_editor_absoluteX="411dp" />

    <TextView
        android:id="@+id/session_details_overlap_start_value"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:layout_marginLeft="24dp"
        android:text="TextView"
        android:textColor="?attr/colorSecondary"
        android:textSize="16sp"
        app:layout_constraintBaseline_toBaselineOf="@+id/session_details_overlap_start_text"
        app:layout_constraintStart_toEndOf="@+id/session_details_overlap_session_barrier" />

    <TextView
        android:id="@+id/session_details_overlap_end_value"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:layout_marginLeft="24dp"
        android:text="TextView"
        android:textColor="?attr/colorSecondary"
        android:textSize="16sp"
        app:layout_constraintBaseline_toBaselineOf="@+id/session_details_overlap_end_text"
        app:layout_constraintStart_toEndOf="@+id/session_details_overlap_session_barrier" />

    <TextView
        android:id="@+id/session_details_overlap_message"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:layout_marginLeft="24dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="24dp"
        android:layout_marginRight="24dp"
        android:text="The set times overlap with an already existing sleep session having these times:"
        android:textColor="?attr/colorOnPrimarySurface"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>